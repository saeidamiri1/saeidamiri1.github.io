<h2 id="arithmetic-operators">Arithmetic Operators</h2>

<table>
  <thead>
    <tr>
      <th>R</th>
      <th>Python</th>
      <th>Matlab</th>
      <th>Description</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>x&lt;-c <br /> x=c</td>
      <td>x=c</td>
      <td>x=c</td>
      <td>define x and assign c to it</td>
    </tr>
    <tr>
      <td>x+y</td>
      <td>x+y</td>
      <td>x+y</td>
      <td>addition</td>
    </tr>
    <tr>
      <td>x-y</td>
      <td>x-y</td>
      <td>x-y</td>
      <td>subtraction</td>
    </tr>
    <tr>
      <td>x*y</td>
      <td>x*y</td>
      <td>x*y</td>
      <td>multiplication</td>
    </tr>
    <tr>
      <td>x/y</td>
      <td>x/y</td>
      <td>x/y</td>
      <td>division</td>
    </tr>
    <tr>
      <td>x^y</td>
      <td>x**y</td>
      <td>x.^y</td>
      <td>power</td>
    </tr>
    <tr>
      <td>x%/%y</td>
      <td>x%y</td>
      <td>rem(x,y)</td>
      <td>Reminder</td>
    </tr>
  </tbody>
</table>

